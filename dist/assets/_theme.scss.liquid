.form-feedback{
	font-size: 10px;
	margin-bottom: 50px;
}


.form-error{
	color: list-get($color,'warning');
}

.form-error-title{
	margin-bottom: 5px;
}

.form-error-list{
	li{
		& + & {
		margin-top: 5px;
		}
	}
}


.form-success{
	color: green;
}



.form-field{

	position: relative;

	&:not(.form-field-no-border){
		border-bottom: 1px solid #eee;
	}


	& + &{
		margin-top: 50px;
	}

	&:not(.form-field-label-inline){

		label:not(.label-in-line){
			position: absolute;
			left:0;
			top:5px;
			@include transition($duration:200ms);

		}

		input:focus ~ label, input:valid ~ label{
			top:-20px;
			font-size:1rem;
		}	
	}




	input{
		width: 100%;
		height: 30px;
	}

	input[type="checkbox"]{
		width: 30px;
		vertical-align: middle;
		-webkit-appearance: checkbox;
		-moz-appearance: checkbox;
	}

	textarea{
		resize: none;
		width: 100%;
		border: 1px solid #eee;
		padding: 5px;

	}

	select{
		width: 100%;
		height: 30px;

		background-image: url("{{ 'icon_dropdown_arrow_black.svg' | asset_url }}");
		background-repeat: no-repeat;
		background-position: 100px 40%;
		background-size: 22px;

	}

	.btn{
		//for ripple effect
		vertical-align: middle;
		@include screen('small-down') {
			margin-bottom: 15px;
		}
	}



}




