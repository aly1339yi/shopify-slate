/* invisible */
@mixin invisible{
  opacity: 0;
  visibility: hidden;
}


/* visible */
@mixin visible{
  opacity: 1;
  visibility: visible;
}


/* max-width */

@mixin max-width($width, $align:null){

  max-width: $width;

  @if ($align){

    @if ($align == 'center') {
      margin-left: auto;
      margin-right: auto;
    }
    @else if ($align == 'right') {
      margin-left: auto;
      margin-right: 0;
    }

  }

}


