/* =========================================================================
  Color
========================================================================= */


$color-purple : #5c6ac4;

$color-black  : #000000;

$color-white  : #ffffff;


$color: (

  'primary'         $color-purple,


  'bullet'          transparent,

  'bullet-selected' $color-black,

  null

);


 /* =========================================================================
   Font Size
   ========================================================================= */

$font-size-normal   : 1.4rem;

$font-size-xx-small : 0.9rem;

$font-size-x-small  : 1.0rem;

$font-size-small    : 1.2rem;

$font-size-medium   : 1.4rem;

$font-size-large    : 1.4rem;

$font-size-x-large  : 1.4rem;

$font-size-xx-large : 1.4rem;

$font-size: (

  'body' $font-size-normal,

  'h1'   1.3rem,

  'h2'   1.4rem,

  'h3'   1.8rem,

  'h4'   2.4rem,

  'h5'   3rem,
  
  'h6'   3rem,

  null

);


 /* =========================================================================
   Responsive
   ========================================================================= */

$breakpoint-small   : 576px;
$breakpoint-medium  : 768px;
$breakpoint-large   : 992px;
$breakpoint-x-large : 1200px;

$breakpoint-nav     : 576px;


$breakpoint-range: (

  'small-down'    '(max-width: #{$breakpoint-small - 1})',
  'small-up'      '(min-width: #{$breakpoint-small})',
  'medium-down'   '(max-width: #{$breakpoint-medium - 1})',
  'medium-up'     '(min-width: #{$breakpoint-medium})',
  'large-down'    '(max-width: #{$breakpoint-large - 1})',
  'large-up'      '(min-width: #{$breakpoint-large})',
  'x-large-down'  '(max-width: #{$breakpoint-x-large - 1})',
  'x-large-up'    '(min-width: #{$breakpoint-x-large})',
  
  'small-medium'  '(min-width: #{$breakpoint-small})  and  (max-width: #{$breakpoint-medium - 1})',
  'medium-large'  '(min-width: #{$breakpoint-medium}) and  (max-width: #{$breakpoint-large - 1})',
  'large-x-large' '(min-width: #{$breakpoint-large})  and  (max-width: #{$breakpoint-x-large - 1})'
  
  null


);

$breakpoint: (

  'small'   576px,
  'medium'  768px,
  'large'   992px,
  'x-large' 1200px,

  'nav'     576px,

  null

);


 /* =========================================================================
   Grid
   ========================================================================= */


$grid-gutter: (

  'normal' 30px,

  null
  
);



 /* =========================================================================
   Z-Index
   ========================================================================= */

$z-index: (

  'navbar'             100,

  'fullscreen-overlay' 200,

  'mini-cart'          300,

  null


);


 /* =========================================================================
   Other
   ========================================================================= */


$navbar-banner-height: (

  'desktop' 20px,

  'mobile'  18px,

  null

);

$navbar-nav-height: (

  'desktop'           80px,

  'desktop-condensed' 40px,

  'mobile'            50px,

  null

);




